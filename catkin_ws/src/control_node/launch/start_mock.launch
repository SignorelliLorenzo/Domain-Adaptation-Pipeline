<launch>
    <include file="$(find kimera_interfacer)/launch/labdata.launch" />

    <node name="label_generator" pkg="label_generator_ros" type="ray_cast_scene.py" output="screen" />

    <node name="mocked_control_node" pkg="control_node" type="mocked_control_node.py" output="screen">
        <param name="image_dir" value="$(find control_node)/mock_data/images" />
        <param name="depth_dir" value="$(find control_node)/mock_data/depths" />
        <param name="gt_label_dir" value="$(find control_node)/mock_data/gt_labels" />
        <param name="dlab_label_dir" value="$(find control_node)/mock_data/deeplab_labels" />
        <param name="pose_dir" value="$(find control_node)/mock_data/poses" />
        <param name="mesh_path" value="/home/michele/Desktop/Domain-Adaptation-Pipeline/Output_kimera_mesh/output_predict_mesh.ply" />
        <param name="serialized_path" value="/home/michele/Desktop/Domain-Adaptation-Pipeline/Output_kimera_mesh/output_serialized.data" />
        <param name="mapping_file" value="$(find control_node)/cfg/nyu40_segmentation_mapping.csv" />
    </node>
</launch>
